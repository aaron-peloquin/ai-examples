{
  "name": "libs-module-library",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "./libs/module-library/libs_module_library",
  "targets": {
    "docs": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pydoc-markdown -p libs_module_library --render-toc > docs/source/api.md",
        "cwd": "./libs/module-library"
      }
    },
    "lock": {
      "executor": "nx:run-commands",
      "options": {
        "command": "poetry lock --no-update",
        "cwd": "./libs/module-library"
      }
    },
    "add": {
      "executor": "@nxlv/python:add",
      "options": {}
    },
    "update": {
      "executor": "@nxlv/python:update",
      "options": {}
    },
    "remove": {
      "executor": "@nxlv/python:remove",
      "options": {}
    },
    "build": {
      "executor": "@nxlv/python:build",
      "outputs": [
        "{projectRoot}/dist"
      ],
      "options": {
        "outputPath": "./libs/module-library/dist",
        "publish": true,
        "lockedVersions": true,
        "bundleLocalDependencies": true
      }
    },
    "install": {
      "executor": "@nxlv/python:install",
      "options": {
        "silent": false,
        "args": "",
        "cacheDir": ".cache/pypoetry",
        "verbose": false,
        "debug": false
      }
    },
    "lint": {
      "executor": "@nxlv/python:flake8",
      "outputs": [
        "{workspaceRoot}/reports/./libs/module-library/pylint.txt"
      ],
      "options": {
        "outputFile": "reports/./libs/module-library/pylint.txt"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": [
        "{workspaceRoot}/reports/./libs/module-library/unittests",
        "{workspaceRoot}/coverage/./libs/module-library"
      ],
      "options": {
        "command": "poetry run pytest tests/",
        "cwd": "./libs/module-library"
      }
    },
    "tox": {
      "executor": "@nxlv/python:tox",
      "outputs": [
        "{workspaceRoot}/reports/./libs/module-library/unittests",
        "{workspaceRoot}/coverage/./libs/module-library"
      ],
      "options": {
        "silent": false,
        "args": ""
      }
    }
  },
  "tags": []
}
