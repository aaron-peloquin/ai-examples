{
  "name": "original",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "./original/original",
  "targets": {
    "main": {
      "executor": "nx:run-commands",
      "options": {
        "command": "python main.py",
        "cwd": "./apps/original"
      }
    },
    "docs": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pydoc-markdown -p original --render-toc > docs/source/api.md",
        "cwd": "./apps/original"
      }
    },
    "lock": {
      "executor": "nx:run-commands",
      "options": {
        "command": "poetry lock --no-update",
        "cwd": "./apps/original"
      }
    },
    "add": {
      "executor": "@nxlv/python:add",
      "options": {}
    },
    "update": {
      "executor": "@nxlv/python:update",
      "options": {}
    },
    "remove": {
      "executor": "@nxlv/python:remove",
      "options": {}
    },
    "build": {
      "executor": "@nxlv/python:build",
      "outputs": [
        "{projectRoot}/dist"
      ],
      "options": {
        "outputPath": "./original/dist",
        "publish": true,
        "lockedVersions": true,
        "bundleLocalDependencies": true
      }
    },
    "install": {
      "executor": "@nxlv/python:install",
      "options": {
        "silent": false,
        "args": "",
        "cacheDir": ".cache/pypoetry",
        "verbose": false,
        "debug": false
      }
    },
    "lint": {
      "executor": "@nxlv/python:flake8",
      "outputs": [
        "{workspaceRoot}/reports/./original/pylint.txt"
      ],
      "options": {
        "outputFile": "reports/./original/pylint.txt"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": [
        "{workspaceRoot}/reports/./original/unittests",
        "{workspaceRoot}/coverage/./original"
      ],
      "options": {
        "command": "poetry run pytest tests/",
        "cwd": "./apps/original"
      }
    },
    "tox": {
      "executor": "@nxlv/python:tox",
      "outputs": [
        "{workspaceRoot}/reports/./original/unittests",
        "{workspaceRoot}/coverage/./original"
      ],
      "options": {
        "silent": false,
        "args": ""
      }
    }
  },
  "tags": []
}
